class TestingCases {

	@Test
	void testFindMax() {
		    assertEquals(4,Cal.findMax(new int[]{1,3,4,2}));  
	        assertEquals(-1,Cal.findMax(new int[]{-12,-1,-3,-4,-2}));  
	}

}
